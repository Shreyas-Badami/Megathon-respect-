create database docnetmd;
use docnetmd;
create table doctors (DId int primary key, Name varchar(100), Specialization varchar(100), Price int, Language varchar(100), Location varchar(100), Insurance varchar(100), PhoneNumber int, NumAppointments int, KarmaPts int, NumCasesSolved int);
create table specializations(Specialization varchar(100), Tag varchar(100), constraint pk_specialization primary key(Specialization, Tag));
create table patients(PId int primary key, Name varchar(100), Sex varchar(1), LinkToCloud varchar(200), Language varchar(100), PhoneNumber int, Location varchar(100), IsServiced varchar(1));
create table appointments(AId int primary key, PId int, DId int, status varchar(1), cost int);
alter table appointments add foreign key (PId) references patients(PId);
alter table appointments add foreign key (DId) references doctors(DId);
